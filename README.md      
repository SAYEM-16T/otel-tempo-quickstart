# 1. Repo clone
git clone git@github.com:your-org/otel-tempo-quickstart.git
cd otel-tempo-quickstart

# 2. Env file create
cp .env.example .env
nano .env   # password change, retention adjust, etc.

# 3. Script executable
chmod +x setup.sh

# 4. Stack up
./setup.sh

# 5. Status check
./setup.sh --status
